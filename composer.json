    "type": "library",
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "luya\\yii\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Shchednyj",
            "email": "admin@adminq.ru"
        }
    ],   
    "config": {
        "fxp-asset": {
